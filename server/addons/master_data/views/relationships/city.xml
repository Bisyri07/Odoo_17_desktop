<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <data>
    <record id='city_relationship_action' model='ir.actions.act_window'>
      <field name='name'>city</field>
      <field name='res_model'>master.city</field>
      <field name='view_mode'>tree,form</field>
    </record>

    <record id='master_city_tree' model='ir.ui.view'>
      <field name='name'>city</field>
      <field name='model'>master.city</field>
      <field name='arch' type='xml'>
        <tree>
          <field name='name'/>
          <field name='country_id'/>
        </tree>
      </field>
    </record>
  </data>
</odoo>