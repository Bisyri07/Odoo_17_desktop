<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

      <record id='uom_relationship_action' model='ir.actions.act_window'>
        <field name='name'>Unit of Measurement</field>
        <field name='res_model'>unit.of.measurement</field>
        <field name='view_mode'>tree,form</field>
      </record>

      <record id='view_uom_tree' model='ir.ui.view'>
        <field name='name'>Unit of Measurement</field>
        <field name='model'>unit.of.measurement</field>
        <field name='arch' type='xml'>
          <tree string='Unit of measurement'>
              <field name='name' />
              <field name='category' />
              <field name='reference_unit' />
              <field name='symbol' />
              <field name='r_t_r_unit' />
              <field name='active' />
          </tree>
        </field>
      </record>

    </data>
</odoo>